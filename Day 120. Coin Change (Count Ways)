class Solution {
    public int count(int coins[], int sum) {
        // code here.
        int[] dp = new int[sum + 1];
        dp[0] = 1; // There is 1 way to make sum 0 (choose nothing)

        for (int coin : coins) {
            for (int j = coin; j <= sum; j++) {
                dp[j] += dp[j - coin];
            }
        }

        return dp[sum];

    }
}
